package com.amrita.java.collections;

import java.util.Hashtable;
import java.util.Map;
import java.util.TreeMap;

public class MapDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Maps
		// HashMap
		Map<String, Integer> ageMap = new Hashtable<String, Integer>();
		// ageMap.put("Tejas", 38);
		ageMap.put("Aarna", 2);
		ageMap.put("Amrita", 36);
		ageMap.put("Tejas", 40);
		ageMap.put("msdnsd", 37);
		System.out.println(ageMap.get("Tejas"));
		System.out.println(ageMap);

		Map<String, Integer> treeMap = new TreeMap<String, Integer>();
		treeMap.put("msdnsd", 37);
		treeMap.put("Aarna", 2);
		treeMap.put("Amrita", 36);
		treeMap.put("Tejas", 40);
	
		System.out.println(treeMap);

	}

}
